
<%= form_with scope: :home, url: home_search_path, class: "form-inline", local: true do |form| %>
<div class="form-group">
    <%= form.label :match %>
    <%= form.select :match, ['first_name', 'last_name'], {}, {class: "form-control"}%>
</div>

<div class="form-group">
    <%= form.label :with %>
    <%= form.text_field :with, class: "form-control" %>
</div>
    <%= form.submit "search", class: "btn btn-default" %>
<% end %>

<%= render 'table' %>