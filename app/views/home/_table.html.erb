<h2>Users</h2>
<table class="table">
  <thread>
    <tr>
        <th scope="col">Avatar</th>
        <th scope="col">First_Name</th>
        <th scope="col">Last_Name</th>
    </tr>
  </thread>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><img src=<%= user.avatar %> alt="..." class="img-circle" style="max-height:70px"></td>
      <td><%= user.first_name %></td>
      <td><%= user.last_name%></td>
    </tr>
  <% end %>
  </tbody>
</table>